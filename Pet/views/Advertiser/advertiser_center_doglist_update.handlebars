          <div class="row bar">
            <div id="customer-account" class="col-lg-9 clearfix">
              <p class="lead">Update your previous posted dog here</p>
              
              <div class="bo3">
              
                <div class="heading">
                  <h3 class="text-uppercase">UPdate Information</h3>
                  <P><strong>Dog ID:</strong></P>
                </div>
                
                <form>

				 <div class="row">
				 	<div class="col-md-6">
				 	<div class="form-group">
        			<div class="file">
        				<i class="ico-plus"></i>Image (only accept jpg/png)<input type="file" name="file" accept="image/*" id="upload" >
        			</div> 
				 	</div>
				 	</div>
				 </div>
                
                  <div class="row">
                                   
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="dogname_add">Dog Name</label>
                        <input id="dogname_add" type="text" class="form-control">
                      </div>
                    </div>
                    
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="dogbreed_add">Dog Breed</label>
                        <input id="dogbreed_add" type="text" class="form-control">
                      </div>
                    </div>
                    
                  </div>
                  <div class="row">
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="dogaddress_add">Address</label>
                        <input id="dogaddress_add" type="text" class="form-control">
                      </div>
                    </div>
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="dogvaccine_add">Vaccine</label>
                        <input id="dogvaccine_add" type="text" class="form-control">
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="dogsize_add">Size</label>                   
                        <select id="dogsize_add" class="form-control">
                  				<option>Small (0-25 lbs)</option>
                  				<option>Medium (26-60 lbs)</option>
                  				<option>Large (60-100 lbs)</option>
                  				<option>Extra Large (101 lbs or more)</option>
                			</select>
                      </div>
                    </div>
                    
                    
                    <div class="col-md-6 col-lg-3">
                      <div class="form-group">
                        <label for="doggender_add">Gender</label>
                        <select id="doggender_add" class="form-control">
                        <option>Male</option>
                        <option>Female</option>
                        </select>
                      </div>
                    </div>
                    <div class="col-md-6 col-lg-3">
                      <div class="form-group">
                        <label for="dogage_add">Age</label>
                        <select id="dogage_add" class="form-control">
                        <option>Puppy</option>
                        <option>Young</option>
                        <option>Adult</option>
                        <option>Senior</option>
                        </select>
                      </div>
                    </div>
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="dogcolor_add">Color</label>
                        <input id="dogcolor_add" type="text" class="form-control">
                      </div>
                    </div>
                    
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="postdate_add">Post Date</label>
                        <input id="postdate_add" type="date" class="form-control"> 
                      </div>
                    </div>
                    
                    <div class="col-md-12">
                      <div class="form-group">
                        <label for="message">Description</label>
                        <textarea id="message" class="form-control"></textarea>
                      </div>
                    </div>
                    
                    <div class="col-md-12 text-center">
                      <button type="submit" class="btn btn-template-outlined"><i class="fa fa-save"></i> Post</button>
                    </div>
                  </div>
                </form>
              </div>
